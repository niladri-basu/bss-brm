// ================================================================================================================================
// ASSOCIATED CAMEL EXTENSION RECORD
// ================================================================================================================================
ASSOCIATED_CAMEL_EXTENSION                                      // optional, 0..1 times
{
  String        RECORD_TYPE;                                    // mandatory, default=700
  Integer       RECORD_NUMBER;                                  // mandatory, auto-generated

  Integer       SERVER_TYPE_OF_NUMBER;                          // optional, but should be defaulted to '0'. Could be used by some 
                                                                // plugins to perform number-normalization.
  String        SERVER_NUMBERING_PLAN;                          // optional
  String        SERVER_ADDRESS;                                 // mandatory!, identifies the server interrogated

  Integer       SERVICE_LEVEL;                                  // mandatory! identifies the level of CAMEL service provided [0-3]
  Integer       SERVICE_KEY;                                    // mandatory! identifies the CAMEL service Logic to be applied 
  Integer       DEFAULT_CALL_HANDLING_INDICATOR;                // optional, indicates whether or not a CAMEL call encountered
                                                                // default handeling (0-Continue the call, 1-Release the call)

  Integer       MSC_TYPE_OF_NUMBER;                             // optional, but should be defaulted to '0'. Could be used by some 
                                                                // plugins to perform number-normalization.
  String        MSC_NUMBERING_PLAN;                             // optional
  String        MSC_ADDRESS;                                    // mandatory! identifies the MSC that generated the CAMEL reference
                                                                // number (may be different from SERVER_ADDRESS)

  String        CAMEL_REFERENCE_NUMBER;                         // mandatory! in association with the MSC_ADDRESS, provides a
                                                                // unique identifier for each CAMEL invocation

  Integer       CAMEL_INITIATED_CF_INDICATOR;                   // optional, but should be defaulted to 0 (1=CAMEL call forwarding)
  String        CAMEL_MODIFICATION_LIST;                        // optional, comma separated string of integers

  Integer       DEST_GSMW_TYPE_OF_NUMBER;                       // optional, but should be defaulted to '0'. Could be used by some 
                                                                // plugins to perform number-normalization.
  String        DEST_GSMW_NUMBERING_PLAN;                       // optional
  String        DEST_GSMW_NUMBER;                               // optional, will be used to identify CAMEL redirection destination
                                                                // (could contain an MSISDN, IP, LOGIN etc.) when the primary
                                                                // extension is of type GSM (ie. ASS_GSMW_EXT)
  String        DEST_GSMW_NUMBER_ORIGINAL;                      // optional, DEST_GSM_NUMBER as received (before normalization)

  String        DEST_GPRS_APN_ADDRESS;                          // optional, (but might be mandatory for specific zoning scenarios,
                                                                // e.g. if an apn_group is used) when the primary extension is of
                                                                // type GPRS (ie. ASS_GPRS_EXT)
  String        DEST_GPRS_PDP_REMOTE_ADDRESS;                   // optional
  
  String        CSE_INFORMATION;                                // optional, the information downloaded by the CAMEL server
  String        GSM_CALL_REFERENCE_NUMBER;                      //optional
  Decimal       EXCHANGE_RATE;                                  // optional, Exchange rate

}
// END OF ASSOCIATED CAMEL EXTENSION RECORD
// ================================================================================================================================


