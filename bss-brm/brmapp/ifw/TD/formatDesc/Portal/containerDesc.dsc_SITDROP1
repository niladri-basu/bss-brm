}

// ================================================================================================================================
// AGGREGATION PERIOD
// ================================================================================================================================
AGGREGATION_PERIOD
{
  String             PERIOD;            // (key) the accounting cycle "YYYYMMDD"
  String             ITEM_POID;         // the item poid the obj_id identifies the period inside portal
  String             CREATED;           // the day on which this element was created
  Decimal            TOTAL_CHARGE;      // the summed up charge
  Decimal            TOTAL_QUANTITY;    // the summed up quantity based on RUM(s) in the discount filter
  Decimal            TOTAL_EVENT;       // the summed up events (based on charge packets)
  Decimal            GRANTED_CHARGE;    // the discounted charge
  Decimal            GRANTED_QUANTITY;  // the discounted quantity
  Decimal            FRAME_CHARGE;      // the summed up charge (based on frame) of the discount frame 
  Decimal            FRAME_QUANTITY;    // the summed up quantity based on RUM(s) in the discount filter of the discount frame
  Decimal            FRAME_EVENT;       // the summed up events (based on charge packets) of the discount frame
}


