VERSION=10056;

// ================================================================================================================================
// HEADER RECORD
// ================================================================================================================================
HEADER                                                          // optional (but mandatory, if a sequence-check or validation 
                                                                // should be performed)
{
  Integer       RECORD_LENGTH;                                  // optional for backward compatibility
  String        RECORD_TYPE;                                    // mandatory, must be set to '010'
  Integer       RECORD_NUMBER;                                  // mandatory, should be set to '1', auto-generated
  String        SENDER;                                         // optional, but should be defaulted if not present on the input 
                                                                // side, e.g. by own NO-Id, i.e. 'DTAG'
  String        RECIPIENT;                                      // optional, but should be defaulted if not present on the 
                                                                // input side, e.g. by own NO-Id, i.e. 'DTAG'
  Integer       SEQUENCE_NUMBER;                                // optional, if no sequence-check is performed. mandatory, if a 
                                                                // sequence-check should be performed
  Integer       ORIGIN_SEQUENCE_NUMBER;                         // optional, will by set with the SEQUENCE_NUMBER by the sequence in
                                                                // the output
  String        SEQ_CHECK_KEY;                                  // optional, if no sequence-check is performed. mandatory, if a 
                                                                // sequence-check for muliple e.g. senders should be performed
  String        SEQ_GEN_KEY;                                    // optional, if no sequence-generation is performed. mandatory, if a 
                                                                // sequence-generation for multiple e.g. senders should be performed
  Date          CREATION_TIMESTAMP;                             // default, default=filesystem-date
  Date          TRANSMISSION_DATE;                              // default, default=sysdate - as the input delivers no time-part 
                                                                // that should be set to 23:59:59
  Date          TRANSFER_CUTOFF_TIMESTAMP;                      // default, default=TRANSMISSION_DATE
  String        UTC_TIME_OFFSET;                                // default, default=<local_time_zone>, e.g. '+0100' for europe
  Integer       SPECIFICATION_VERSION_NUMBER;                   // default, default='04'
  Integer       RELEASE_VERSION;                                // default, default='03'
  String        ORIGIN_COUNTRY_CODE;                            // optional, should be default=<iac><cc> of country where the 
                                                                // transaction originated (if not present on the input side), e.g.
                                                                // '0049' for germany
  String        SENDER_COUNTRY_CODE;                            // optional, should be default=<iac><cc> of country where the 
                                                                // transaction was sent to (if not present on the input side), e.g.
                                                                // '0049' for germany
  String        DATA_TYPE_INDICATOR;                            // mandatory, default=<space> for chargable data
  String        IAC_LIST;                                       // Comma separated list of possible IACs.
  String        CC_LIST;                                        // Comma separated list of possible CCs.
  Integer       TAP_DECIMAL_PLACES;                             // optional, but mandatory for RAP output, will be set by the
                                                                // input grammar
  String        OPERATOR_SPECIFIC_INFO;                         // optional, default='' (used to store a key to be able to uniquely
                                                                // identify the CDR used to generate a specific EDR - usefull for
                                                                // RAP or CIBER return), must be set by a user iScript if needed.
  String        CIBER_FILLER;                                   // optionnal,

  String        CIBER_RECORD_TYPE;                              // optionnal,See Ciber v2.5 specs for usage
  String        RETURN_INDICATOR;                               // optionnal,See Ciber v2.5 specs for usage
  String        CURRENCY;                                       // optionnal,See Ciber v2.5 specs for usage
  String        SETTLEMENT_PERIOD;                              // optionnal,See Ciber v2.5 specs for usage
  String        CLEARINGHOUSE_ID;                               // optionnal,See Ciber v2.5 specs for usage
  String        BATCH_REJECT_REASON;                            // optionnal,See Ciber v2.5 specs for usage
  String        BATCH_CONTENTS;                                 // optionnal,See Ciber v2.5 specs for usage
  String        SENDING_CLEARINGHOUSE_BID;                      // optionnal,See Ciber v2.5 specs for usage
  String        CREATION_PROCESS;                               // Process that created output stream. RATING_PIPELINE, 
                                                                // EVENT_EXTRACTION_TOOL, BACKOUT_PIPELINE, RERATING_PIPELINE,
                                                                // or PIN_TRANSFORM_CDR.
  String        SCHEMA_VERSION;                                 // Version number for schema.
  String        EVENT_TYPE;                                     // Portal event type.
  String        RAP_FILE_SEQ_NO;                                // optional  - RapFileSequenceNumber item
  String        QUERYABLE_FIELDS_MAPPING;                       // optional, but might be mandatory-calculated for suspense
                                                                // handling.  Database column names and data types that map
                                                                // to queryable fields.  It has the format as
                                                                // <column_name>:<data_type>[;<column_name>:<data_type>[;...]]
  String        BATCH_ID;                                       // optional, set equal to the actual file batch id
  String        UTC_END_TIME_OFFSET;                            //optional
  String        OVERRIDE_REASONS;                               // optional, used only when resubmission of batch, with override
                                                                // of certain batch level suspense reasons
  String        TAP_PROCESSING_INFO;                            // optional, used only when resubmission of batch, for
                                                                // passing tap specific info populated by TAP/RAP plug-ins
                                                                // in a suspended batch record

  Integer       BATCH_CTRL_INFO_START_INDEX;                    // BatchControlInfo block start index
  Integer       BATCH_CTRL_INFO_END_INDEX;                      // BatchControlInfo block end index
  Integer       ACCOUNTING_INFO_START_INDEX;                    // AccountingInfo block start index
  Integer       ACCOUNTING_INFO_END_INDEX;                      // AccountingInfo block end index
  Integer       NETWORK_INFO_START_INDEX;                       // NetworkInfo block start index
  Integer       NETWORK_INFO_END_INDEX;                         // NetworkInfo block end index
  Integer       MESSAGE_DESCRIPTION_START_INDEX;                // MessageDescriptionInfoList block start index
  Integer       MESSAGE_DESCRIPTION_END_INDEX;                  // MessageDescriptionInfoList block end index
  Integer       NOTIFICATION_START_INDEX;                       // Notification block start index
  String        DELAYED_ERROR_BLOCK;				// Block having the error
  Integer       OBJECT_CACHE_TYPE;                              // 0: Convergent; 1: Prepaid; 2: Postpaid.
  String        TAP_FILE_TYPE;
  
  
  ASSOCIATED_TAP3_ERROR		ASS_TAP3_ERR;
  ASSOCIATED_ROAMING_EXTENSION        ASS_ROAMING_EXT;
}


// ================================================================================================================================
// BASIC DETAIL RECORD
// ================================================================================================================================
DETAIL                                                          // mandatory
{
  Integer       RECORD_LENGTH;                                  // optional for backward compatibility
  String        RECORD_TYPE;                                    // mandatory, must be within the defined range (020-089, 100-299)
  Integer       RECORD_NUMBER;                                  // mandatory, auto-generated
  Integer       DISCARDING;                                     // mandatory, default=0
  String        CHAIN_REFERENCE;                                // optional (but mandatory, if an assembling plugin should 
                                                                // analyse first-intermediate-last records)
  String        SOURCE_NETWORK_TYPE;                            // optional
  String        SOURCE_NETWORK;                                 // optional (but might be mandatory in order to recognize specific 
                                                                // reseller, roaming or interconnect scenarios, e.g. if the NOSP 
                                                                // plugin or CarrierICRating is used)
  String        DESTINATION_NETWORK_TYPE;                       // optional
  String        DESTINATION_NETWORK;                            // optional (but might be mandatory in order to recognize specific 
                                                                // reseller, roaming or interconnect scenarios, e.g. if the NOSP 
                                                                // plugin or CarrierICRating is used)
  String        TYPE_OF_A_IDENTIFICATION;                       // optional
  String        A_MODIFICATION_INDICATOR;                       // optional
  Integer       A_TYPE_OF_NUMBER;                               // optional, but should be defaulted to '0'. Could be used by some 
                                                                // plugins to perform number-normalization.
  String        A_NUMBERING_PLAN;                               // optional
  String        A_NUMBER;                                       // mandatory!, will be used to identify the billable party (could 
                                                                // contain an MSISDN, IP, LOGIN etc.). If not present on the input 
                                                                // side: use any default-value !
  String        B_MODIFICATION_INDICATOR;                       // optional-calculated, might be used to identify Social-Numbers
  Integer       B_TYPE_OF_NUMBER;                               // optional, but should be defaulted to '0'. Could be used by some 
                                                                // plugins to perform number-normalization.
  String        B_NUMBERING_PLAN;                               // optional
  String        B_NUMBER;                                       // mandatory!, will be used to identify the other party (could 
                                                                // contain an MSISDN, IP, LOGIN etc.). If not present on the input 
                                                                // side: use a default-value, e.g. 0049 !
  String        DESCRIPTION;                                    // calculated
  String        C_MODIFICATION_INDICATOR;                       // optional
  Integer       C_TYPE_OF_NUMBER;                               // optional, but should be defaulted to '0'. Could be used by some 
                                                                // plugins to perform number-normalization.
  String        C_NUMBERING_PLAN;                               // optional
  String        C_NUMBER;                                       // optional, but might be used in order to be able to detect 
                                                                // certain usage scenarios
  String        USAGE_DIRECTION;                                // mandatory, default=0
  String        CONNECT_TYPE;                                   // optional, but might be used in order to be able to detect 
                                                                // certain usage scenarios
  String        CONNECT_SUB_TYPE;                               // optional, but might be used in order to be able to detect 
                                                                // certain usage scenarios
  String        CALLED_COUNTRY_CODE;                            // optional, but should be set when the CONNECT_TYPE indicates 
                                                                // an international call
  String        BASIC_SERVICE;                                  // mandatory!, default='011', will be used to identify which 
                                                                // service had been used, which is one of the primary attributes 
                                                                // which guides rating !
  String        QOS_REQUESTED;                                  // calculated, but might be used in order to be able to detect 
                                                                // certain usage scenarios
  String        QOS_USED;                                       // calculated, but might be used in order to be able to detect 
                                                                // certain usage scenarios
  String        CALL_COMPLETION_INDICATOR;                      // default, but should be defaulted with 'C' or '00'
  String        LONG_DURATION_INDICATOR;                        // mandatory, default='S' (might be used for assembling events 
                                                                // together with the CHAIN_REFERENCE)
  Date          CHARGING_START_TIMESTAMP;                       // mandatory!
  Date          CHARGING_END_TIMESTAMP;                         // optional, might be used to calculate the DURATION
  Date          CREATED_TIMESTAMP;                              // optional, the time that the event was created in Portal
  String        UTC_TIME_OFFSET;                                // default, default=<local_time_zone>, e.g. '+0100' for europe
  Decimal       DURATION;                                       // mandatory!, default=0 - should be set for all time-based 
  Integer       TOTAL_CALL_EVENT_DURATION;                      // mandatory!, default=0 - should be set for all time-based 
                                                                // services, e.g. for telephony
  String        DURATION_UOM;                                   // mandatory, default='SEC'
  Decimal       VOLUME_SENT;                                    // mandatory!, default=0 - should be set for all volume-based 
                                                                // services, e.g. for gprs
  String        VOLUME_SENT_UOM;                                // mandatory, default='BYT'
  Decimal       VOLUME_RECEIVED;                                // mandatory!, dafault=0 - should be set for all volume-based 
                                                                // services, e.g. for gprs
  String        VOLUME_RECEIVED_UOM;                            // mandatory, default='BYT'
