LERG_DATA
{
  Decimal       LONGITUDE;            // vertical coordinate
  Decimal       LATITUDE;             // horizontal coordinate
  String        LATA_SUBZONE;         // Finer LATA breakdown (Florida only) (2 numerics).
  String        LATA;                 // Local Access Transport Area (3 numerics), may be 000 in certain cases.
  String        LATA_NAME;            // Descriptive name.
  String        IDDD_IND;             // 0 = indicates that International Direct Distance Dialing (IDDD)
                                      // cannot originate from the NPA NXX.  1 = indicates it can.
  String        TBPI;                 // Thousands Block Pooling Indicator
  String        DIALABLE_IND;         // 0: indicates the NPA NXX cannot be directly dialed by a subscriber. 1 = indicates it can.
  String        NPA_NONMEX;           // Actual NPA for Non-Mexican "Other Place"
  String        NPA;                  // Numbering Plan Area (Area Code)
  String        NXX;                  // NXX (N=2-9, X=0-9) (also called exchange, prefix and Central Office (CO) Code). Note
                                      // that NXXTYPEs 14 will have N=0,1 and NXXTYPE 17 will have N=0-9.
  String        NXX_TYPE;             // Provides information on how the NXX is being used.
  String        PORTABLE_IND;         // TPM Master, Activity, and Minor VH files only.
                                      // 0 = Lines in the NXX have not been designated to be "portable" to another company.
                                      // 1 = The NXX has been identified as one which other companies can "port" numbers from.
  String        SEND_TO_RAO;          // The Revenue Accounting Office to which the Centralized Message Distribution System (CMDS)
                                      // will "send" data.  See "Bill to RAO".
  String        OTHER_LINE_RATE_STEP; // Coded information needed to properly determine the rating associated in certain non-US
                                      // areas. In the case of Mexican data, this pertains to bands 1-8.
  String        OCN;                  // A four digit code that refers to the party owning or responsible for a given record.
                                      // 0000 indicates unknown or not applicable.
  String        BILL_TO_RAO;          // The "bill to" Revenue Accounting Office (RAO). In V&H Data NXX files, if this is
                                      // an "additional" RAO, the "main" RAO will be in the "Send to RAO" field.
  String        SPECIAL_RATING_IND;   // 0 = No Special Rating, 1 = Person Day Rate Only, 2 = No Collect Calls,
                                      // 3 = Collect Calls Person Only
  String        TIMEZONE;             // 0 = Not applicable, 1 = Guam and the Commonwealth of the Northern Mariana Islands (CNMI),
                                      // 2 = Hawaii, 3 = Alaska/Yukon, 4 = Pacific, 5 = Mountain, 6 = Central, 7 = Eastern,
                                      // 8 = Atlantic, 9 = Newfoundland
  String        DSO_IND;              // 0 = Not observed, 1 = Observed, for the given NXX.Note: For NXXTYPEs 14 and 17, although
                                      // a value may exist in the field, Daylight Savings observance is not a factor and the
                                      // field does not apply.
  String        STATE;                // Standardized two-character code for US states. Other areas are based on the two carrier
                                      // identifier used by COMMON LANGUAGE. due to correlate these records with other telephony
                                      // data which may or may not correlate with standard postal abbreviations.
  String        COMPANY_CODE;         // A code equaling the last two digits of the Bell Operating Company (BOC) operating in
                                      // the area of the assigned NPA NXX.  Various locations and NXXTYPEs default this to 00.
  String        COMPANY_TYPE;         // Company Type (0-9)
  String        POINT_ID;             // Point Identification (0, 2-6, 9, B, C)
  String        SSC;                  // Special Service Code (SSC)
  String        TD_EO;                // Terminating Digits End Office
  String        TD_AT;                // Terminating Digits Access Tandem
  String        AOCN;                 // Administrative OCN
  String        LOC_NAME;             // Locality Name
  String        LOC_COUNTY;           // Locality County
  String        RC_ABBREV;            // Rate Center (RC) Name Abbreviation
  String        RC_TYPE;              // Rate Center Type
  String        SWITCH_NAME;          // Switch
  String        SHA_IND;              // Switch Homing Arrangement (SHA) Indicator
  String        RANGE_OCN;            // Operating Company Number (OCN) for range
}
