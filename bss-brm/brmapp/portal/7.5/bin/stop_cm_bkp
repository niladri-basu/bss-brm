#

#
# stop_cm
#
#
VERSION=7.5
PATH=/usr/bin:/bin

CM=cm
LOGDIR=/brmapp/portal/7.5/var/cm
CMPID=${LOGDIR}/cm.pid

me=$0

if [ ! -f ${CMPID} ]; then
	echo "${me}: ERROR: no pid file: ${CMPID}, cannot kill \"${CM}\""
        exit 1
fi       

kill `cat ${CMPID}`
rc=$?
if [ ${rc} -ne 0 ]; then
	echo "${me}: ERROR: kill ${CM} `cat ${CMPID}` FAILED"
	exit ${rc}
else
	rm ${CMPID}
fi

exit 0
