# stop_js
#
#
VERSION=7.5

LOGDIR=/brmapp/portal/7.5/var/eai_js
JSPID=${LOGDIR}/eai_js.pid

me=$0

if [ ! -f ${JSPID} ]; then
	echo "${me}: ERROR: no pid file: ${JSPID}, cannot kill \"${JS}\""
        exit 1
fi       

kill `cat ${JSPID}`
rc=$?
if [ ${rc} -ne 0 ]; then
	echo "${me}: ERROR: kill ${JS} `cat ${JSPID}` FAILED"
	exit ${rc}
else
	rm ${JSPID}
fi

exit 0
