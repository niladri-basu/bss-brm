<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
<!--  price_list definition  -->
	<xs:element name="price_list">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="plan_list" type="plan_listType"/>
				<xs:element name="plan" type="planType"/>
				<xs:element name="best_pricing" type="bestpricingType"/>
				<xs:element name="deal" type="dealType"/>
				<xs:element name="product" type="productType"/>
				<xs:element name="discount" type="discountType"/>
				<xs:element name="sponsorship" type="sponsorshipType"/>
				<xs:element name="dependency" type="dependencyType"/>
				<xs:element name="transition" type="transitionType"/>
				<xs:element name="zonemaps" type="zonemapsType"/>
			</xs:choice>
			<xs:attribute name="version" type="xs:string" default="7.4"/>
		</xs:complexType>
		<xs:unique name="plan_name">
			<xs:selector xpath="plan"/>
			<xs:field xpath="plan_name"/>
		</xs:unique>
		<xs:unique name="deal_name">
			<xs:selector xpath="deal"/>
			<xs:field xpath="deal_name"/>
		</xs:unique>
		<xs:unique name="product_name">
			<xs:selector xpath="product"/>
			<xs:field xpath="product_name"/>
		</xs:unique>
		<xs:unique name="discount_name">
			<xs:selector xpath="discount"/>
			<xs:field xpath="discount_name"/>
		</xs:unique>
		<xs:unique name="sponsorship_name">
			<xs:selector xpath="sponsorship"/>
			<xs:field xpath="sponsorship_name"/>
		</xs:unique>
		<xs:unique name="zonemap_name">
			<xs:selector xpath="zonemaps"/>
			<xs:field xpath="zonemap_name"/>
		</xs:unique>
	</xs:element>
<!--  PLANLIST definition  -->
	<xs:complexType name="plan_listType">
		<xs:sequence>
			<xs:element ref="plan_list_name"/>
			<xs:element ref="plan_list_code" minOccurs="0"/>
			<xs:element ref="description" minOccurs="0"/>
			<xs:element ref="plan_name" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="type" default="new"/>
		<xs:attribute name="status" default="active">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="active"/>
					<xs:enumeration value="inactive"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
<!--  PLAN definition  -->
	<xs:complexType name="planType">
		<xs:sequence>
			<xs:element ref="plan_name"/>
			<xs:element ref="plan_code" minOccurs="0"/>
			<xs:element ref="description" minOccurs="0"/>
			<xs:element ref="deal_name" minOccurs="0"/>
			<xs:element name="credit_limit" type="credit_limitType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="consumption_rule" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="service_deal" type="service_dealType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="bal_info" type="bal_infoType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="ondemand_billing" default="no">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes"/>
					<xs:enumeration value="no"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
<!--  Best Pricing definition  -->
 <xs:complexType name="bestpricingType">
	<xs:sequence>
		<xs:element ref="deal_name"/>
		<xs:element ref="deal_code" minOccurs="0"/>
		<xs:element name="bp_deal" type="bp_dealType" minOccurs="1" maxOccurs="unbounded"/>
	</xs:sequence>
	<xs:attribute name="apply_mode" default="adjustment">
		<xs:simpleType>
			<xs:restriction base="xs:NMTOKEN">
				<xs:enumeration value="rerate"/>
				<xs:enumeration value="adjustment"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	</xs:complexType>
	<xs:complexType name="bp_dealType">
		<xs:sequence>
			<xs:element ref="deal_name"/>
			<xs:element ref="deal_code" minOccurs="0"/>
			<xs:element ref="minimum" minOccurs="0"/>
			<xs:element name="bp_condition" type="bp_conditionType" minOccurs="0"  maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="bp_conditionType">
		<xs:sequence>
			<xs:element ref="resource_id"/>
			<xs:element ref="amount"/>
		</xs:sequence>
		<xs:attribute name="operator" default="equal">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="equal"/>
					<xs:enumeration value="notequal"/>
					<xs:enumeration value="lessthan"/>
					<xs:enumeration value="greatthan"/>
					<xs:enumeration value="notlessthan"/>
					<xs:enumeration value="notgreatthan"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
<!--  DEAL definition  -->
	<xs:complexType name="dealType">
		<xs:sequence>
			<xs:element ref="deal_name"/>
			<xs:element ref="deal_code" minOccurs="0"/>
			<xs:element ref="description" minOccurs="0"/>
			<xs:element name="start_time" type="start_end_timeType" minOccurs="0"/>
			<xs:element name="end_time" type="start_end_timeType" minOccurs="0"/>
			<xs:element ref="permitted" minOccurs="0"/>
			<xs:element name="deal_product" type="deal_productType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="deal_discount" type="deal_discountType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="customization_flag" default="optional">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="optional"/>
					<xs:enumeration value="required"/>
					<xs:enumeration value="prohibited"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="ondemand_billing" default="no">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes"/>
					<xs:enumeration value="no"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="grant_resources_as_group" default="no">
                        <xs:simpleType>
                                <xs:restriction base="xs:NMTOKEN">
                                        <xs:enumeration value="yes"/>
                                        <xs:enumeration value="no"/>
                                </xs:restriction>
                        </xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="deal_array_elemType">
		<xs:simpleContent>
			<xs:extension base = "xs:string">
				<xs:attribute name="type" default="required">
					<xs:simpleType>
						<xs:restriction base="xs:NMTOKEN">
							<xs:enumeration value="optional"/>
							<xs:enumeration value="required"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="deal_discountType">
		<xs:sequence>
			<xs:element ref="discount_name"/>
			<xs:element ref="discount_code"/>
			<xs:element ref="quantity" minOccurs="0"/>
			<xs:element name="purchase_start" type="purchase_startType" minOccurs="0"/>
			<xs:element name="purchase_end" type="purchase_endType" minOccurs="0"/>
			<xs:element name="cycle_start" type="cycle_startType" minOccurs="0"/>
			<xs:element name="cycle_end" type="cycle_endType" minOccurs="0"/>
			<xs:element name="usage_start" type="usage_startType" minOccurs="0"/>
			<xs:element name="usage_end" type="usage_endType" minOccurs="0"/>
			<xs:element ref="status_flag" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="status" default="active">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="active"/>
					<xs:enumeration value="inactive"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="deal_productType">
		<xs:sequence>
			<xs:element ref="product_name"/>
			<xs:element ref="product_code"/>
			<xs:element ref="quantity" minOccurs="0"/>
			<xs:element name="purchase_start" type="purchase_startType" minOccurs="0"/>
			<xs:element name="purchase_end" type="purchase_endType" minOccurs="0"/>
			<xs:element ref="purchase_discount" minOccurs="0"/>
			<xs:element name="cycle_start" type="cycle_startType" minOccurs="0"/>
			<xs:element name="cycle_end" type="cycle_endType" minOccurs="0"/>
			<xs:element ref="cycle_discount" minOccurs="0"/>
			<xs:element name="usage_start" type="usage_startType" minOccurs="0"/>
			<xs:element name="usage_end" type="usage_endType" minOccurs="0"/>
			<xs:element ref="usage_discount" minOccurs="0"/>
			<xs:element ref="status_flag" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="status" default="active">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="active"/>
					<xs:enumeration value="inactive"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
<!--  PRODUCT definition  -->
	<xs:complexType name="productType">
		<xs:sequence>
			<xs:element ref="product_name"/>
			<xs:element ref="product_code"/>
			<xs:element ref="priority" minOccurs="0"/>
			<xs:element ref="description" minOccurs="0"/>
			<xs:element name="start_time" type="start_end_timeType" minOccurs="0"/>
			<xs:element name="end_time" type="start_end_timeType" minOccurs="0"/>
			<xs:element ref="purchase_max" minOccurs="0"/>
			<xs:element ref="purchase_min" minOccurs="0"/>
			<xs:element ref="own_max" minOccurs="0"/>
			<xs:element ref="own_min" minOccurs="0"/>
			<xs:element ref="permitted" minOccurs="0"/>
			<xs:element ref="provisioning" minOccurs="0"/>
			<xs:element ref="tax_supplier" minOccurs="0"/>
			<xs:element ref="zonemap_name" minOccurs="0"/>
			<xs:element name="event_rating_map" type="event_rating_mapType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="rollover" type="rolloverType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="rum_map" type="rum_mapType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="type" default="subscription">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="item"/>
					<xs:enumeration value="subscription"/>
					<xs:enumeration value="system"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="partial" default="no">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes"/>
					<xs:enumeration value="no"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="partial_update" default="no" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes"/>
					<xs:enumeration value="no"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
<!--  DISCOUNT definition  -->
	<xs:complexType name="discountType">
		<xs:sequence>
			<xs:element ref="discount_name"/>
			<xs:element ref="discount_code"/>
			<xs:element ref="priority" minOccurs="0"/>
			<xs:element ref="description" minOccurs="0"/>
			<xs:element name="start_time" type="start_end_timeType" minOccurs="0"/>
			<xs:element name="end_time" type="start_end_timeType" minOccurs="0"/>
			<xs:element ref="purchase_max" minOccurs="0"/>
			<xs:element ref="purchase_min" minOccurs="0"/>
			<xs:element ref="own_max" minOccurs="0"/>
			<xs:element ref="own_min" minOccurs="0"/>
			<xs:element ref="permitted" minOccurs="0"/>
			<xs:element ref="provisioning" minOccurs="0"/>
			<xs:element name="discount_usage_validity" type="discount_usage_validityType" minOccurs="0"/>
			<xs:element name="discount_cycle_validity" type="discount_cycle_validityType" minOccurs="0"/>
			<xs:element name="discount_usage_map" type="discount_usage_mapType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="type" default="subscription">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="item"/>
					<xs:enumeration value="subscription"/>
					<xs:enumeration value="system"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="mode" default="parallel">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="parallel"/>
					<xs:enumeration value="cascading"/>
					<xs:enumeration value="sequential"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="discount_cycle_validityType">
		<xs:attribute name="discount_from_validity_flags" default="prorate">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="full"/>
					<xs:enumeration value="prorate"/>
					<xs:enumeration value="no"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="discount_to_validity_flags" default="prorate">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="full"/>
					<xs:enumeration value="prorate"/>
					<xs:enumeration value="no"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="discount_partial_validity_flags" default="prorate">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="full"/>
					<xs:enumeration value="prorate"/>
					<xs:enumeration value="no"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="discount_usage_mapType">
		<xs:sequence>
			<xs:element ref="event_type"/>
			<xs:choice>
				<xs:element ref="discount_model"/>
				<xs:element ref="model_selector"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="discount_flags" type="xs:NMTOKENS"/>
		<xs:attribute name="snowball_flag" default="yes">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes"/>
					<xs:enumeration value="no"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="discount_usage_validityType">
		<xs:attribute name="discount_from_validity_flags" default="prorate">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="full"/>
					<xs:enumeration value="prorate"/>
					<xs:enumeration value="no"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="discount_to_validity_flags" default="prorate">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="full"/>
					<xs:enumeration value="prorate"/>
					<xs:enumeration value="no"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="discount_partial_validity_flags" default="prorate">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="full"/>
					<xs:enumeration value="prorate"/>
					<xs:enumeration value="no"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
<!--  SPONSORSHIP definition  -->
	<xs:complexType name="sponsorshipType">
		<xs:sequence>
			<xs:element ref="sponsorship_name"/>
			<xs:element ref="sponsorship_code" minOccurs="0"/>
			<xs:element ref="description" minOccurs="0"/>
			<xs:element name="start_time" type="start_end_timeType" minOccurs="0"/>
			<xs:element name="end_time" type="start_end_timeType" minOccurs="0"/>
			<xs:element name="sponsorship_usage_map" type="sponsorship_usage_mapType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="sponsorship_usage_mapType">
		<xs:sequence>
			<xs:element ref="event_type"/>
			<xs:element ref="sponsorship_model"/>
		</xs:sequence>
		<xs:attribute name="sponsorship_flags" type="xs:NMTOKENS"/>
	</xs:complexType>
<!--  DEPENDENCY definition  -->
	<xs:complexType name="dependencyType">
		<xs:sequence>
			<xs:element name="dependent" type="dependentType"/>
			<xs:element ref="dependent_code" minOccurs="0"/>
			<xs:element name="dependee" type="dependeeType"/>
			<xs:element ref="dependee_code" minOccurs="0"/>
			<xs:element ref="delete" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="type" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="dependentType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="type" type="xs:string" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="dependeeType">
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="type" type="xs:string" use="required"/>
				</xs:extension>
			</xs:simpleContent>
	</xs:complexType>
<!--  TRANSITION definition  -->
	<xs:complexType name="transitionType">
		<xs:sequence>
			<xs:element name="from" type="fromType"/>
			<xs:element ref="from_code" minOccurs="0"/>
			<xs:element name="to" type="toType"/>
			<xs:element ref="to_code" minOccurs="0"/>
			<xs:element name="start_time" type="start_end_timeType" minOccurs="0"/>
			<xs:element name="end_time" type="start_end_timeType" minOccurs="0"/>
			<xs:element ref="delete" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="type" type="xs:string" use="required"/>
		<xs:attribute name="service_type" type="xs:string" use="required"/>
		<xs:attribute name="fee_flag" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="fromType">
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="type" type="xs:string" use="required"/>
				</xs:extension>
			</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="toType">
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="type" type="xs:string" use="required"/>
				</xs:extension>
			</xs:simpleContent>
	</xs:complexType>
<!-- ZONEMAP definition  -->
	<xs:complexType name="zonemapsType">
		<xs:sequence>
			<xs:element ref="zonemap_name"/>
			<xs:element ref="description" minOccurs="0"/>
			<xs:element name="zonemap_info" type="zonemap_infoType"/>
		</xs:sequence>
		<xs:attribute name="zonemap_data_type" default="flist">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="flist"/>
					<xs:enumeration value="xml"/>
					<xs:enumeration value="binary"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="zonemap_search_type" default="exact">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="prefix"/>
					<xs:enumeration value="exact"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="zonemap_infoType">
		<xs:sequence>
			<xs:element name="zonemap_data_set" type="zonemap_data_setType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="zonemap_zones" type="zonemap_zonesType" minOccurs="0" maxOccurs="unbounded"/> 
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zonemap_data_setType">
		<xs:sequence>
			<xs:element ref="zonemap_data_data" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zonemap_zonesType">
		<xs:sequence>
			<xs:element ref="zone_name" minOccurs="0"/>
			<xs:element name="zonemap_data_set" type="zonemap_data_setType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="zonemap_zones" type="zonemap_zonesType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
<!--  ROLLOVER definition  -->
	<xs:complexType name="rolloverType">
		<xs:sequence>
			<xs:element ref="rollover_name"/>
			<xs:element ref="rollover_code" minOccurs="0"/>
			<xs:element ref="description" minOccurs="0"/>
			<xs:element name="start_time" type="start_end_timeType" minOccurs="0"/>
			<xs:element name="end_time" type="start_end_timeType" minOccurs="0"/>
			<xs:element name="relative_start_offset" type="relative_start_offsetType" minOccurs="0"/>
			<xs:element name="relative_end_offset" type="relative_end_offsetType" minOccurs="0"/>
			<xs:element name="balance_impact" type="rollover_bal_impactType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="relative_start_unit" default="second">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="second"/>
					<xs:enumeration value="minute"/>
					<xs:enumeration value="hour"/>
					<xs:enumeration value="day"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="relative_end_unit" default="second">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="second"/>
					<xs:enumeration value="minute"/>
					<xs:enumeration value="hour"/>
					<xs:enumeration value="day"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
