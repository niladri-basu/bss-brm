#======================================================================
# G/L ID for National Airtime other GSM
#======================================================================
glid (
	id		1440
	descr		National Airtime other GSM
	taxcode		NORM
	gl_acct	billed			gross	wireless.debit	wireless.credit
	gl_acct	billed			net	wireless.debit	wireless.credit
	gl_acct	billed			disc	wireless.credit	wireless.debit
	gl_acct	billed_earned		gross	wireless.debit	wireless.credit
	gl_acct	billed_earned		net	wireless.debit	wireless.credit
	gl_acct	billed_earned		disc	wireless.credit	wireless.debit
	gl_acct	unbilled 		gross 	wireless.debit	wireless.credit
